<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Strict//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-strict.dtd">
<html xmlns="http://www.w3.org/1999/xhtml">
<head>
	<title>Tools - English</title>
	<meta charset="utf-8"/>

	<style>
		body { font-family: sans-serif; background: #ddd; margin: 20px; }
		body div { display: inline-block; background: #fff; margin: 4px; padding: 8px; }
		body div span { display: block; }
		body div img { width: 100px; }
	</style>

</head>
<body>

<div id="home">
	<span>Home</span>
	<img src="../home.svg" />
</div>

<div id="languages">
	<span>Languages</span>
	<img src="../languages.svg" />
</div>

<div id="back">
	<span>Step Back</span>
	<img src="../back.svg" />
</div>

<div id="replay">
	<span>Repeat Last Step</span>
	<img src="../replay.svg" />
</div>

<div id="forward">
	<span>Next Step</span>
	<img src="../forward.svg" />
</div>

<div id="layerA">
	<span>Expand Layer A</span>
	<img src="../layerA.svg" />
</div>

<div id="layerB">
	<span>Expand Layer B</span>
	<img src="../layerB.svg" />
</div>

<div id="layerC">
	<span>Expand Layer C</span>
	<img src="../layerC.svg" />
</div>

<div id="layerD">
	<span>Expand Layer D</span>
	<img src="../layerD.svg" />
</div>

<div id="clip">
	<span>Change Selection (x,y,z)</span>
	<img src="../clip.svg" />
</div>

<div id="clipReset">
	<span>Reset Selection</span>
	<img src="../clipReset.svg" />
</div>

<div id="clipFading">
	<span>Fading Width</span>
	<img src="../clipFading.svg" />
</div>

<div id="clipTransp">
	<span>Deselection Transparency</span>
	<img src="../clipTransp.svg" />
</div>

<div id="viewReset">
	<span>Reset View</span>
	<img src="../viewReset.svg" />
</div>

<div id="highlight">
	<span>Highlight Moving Elements</span>
	<img src="../highlight.svg" />
</div>

<div id="select">
	<span>Pointing at Elements</span>
	<img src="../select.svg" />
</div>

<div id="visMS">
	<span>Visibility Moving Sheets</span>
	<img src="../visMS.svg" />
</div>

<div id="visMP">
	<span>Visibility Moving Pins</span>
	<img src="../visMP.svg" />
</div>

<div id="visSS">
	<span>Visibility Static Sheets</span>
	<img src="../visSS.svg" />
</div>

<div id="visSP">
	<span>Visibility Static Pins</span>
	<img src="../visSP.svg" />
</div>

<div id="visIS">
	<span>Visibility Intermediate Sheets</span>
	<img src="../visIS.svg" />
</div>

